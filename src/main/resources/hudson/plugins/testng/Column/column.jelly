<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout"
         xmlns:t="/lib/hudson" xmlns:f="/lib/form" xmlns:bh="/lib/health">
    <j:set var="testResultString" value="${it.getTestResultAction(job)}"/>

    <td>
        <j:choose>
             <j:when test="${it.totalCount==0}">
             (${%no tests})
             </j:when>
             <j:otherwise>
               ${testResultString}
             </j:otherwise>
    </td>
</j:jelly>
